{
  "name": "pomo",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "clean": "rimraf dist",
    "build": "yarn run clean && webpack-dashboard -- webpack --config config/webpack.config.js --env.build --env.sourceMap",
    "dev": "webpack-dev-server --config config/webpack.config.js --open",
    "dev:dashboard": "webpack-dashboard -m -- webpack-dev-server --config config/webpack.config.js --open",
    "lint": "tslint --project tsconfig.json",
    "tsc": "tsc -p . --noEmit",
    "tsc:watch": "tsc -p . --noEmit -w",
    "test": "jest --config jest.json",
    "test:watch": "jest --config jest.json --watch",
    "test:update": "jest --config jest.json -u",
    "reinstall": "rm -rf node_modules && yarn",
    "reinstall:win": "rd /s /q node_modules && yarn",
    "precommit": "yarn run lint",
    "prepush": "yarn run lint & yarn run tsc & yarn run test",
    "deploy": "echo 'deploy not set!'"
  },
  "dependencies": {
    "connected-react-router": "^4.3.0",
    "express": "^4.16.2",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.2.2",
    "recompose": "^0.26.0",
    "redux": "^3.7.2",
    "redux-saga": "^0.16.0",
    "reselect": "^3.0.1",
    "tslib": "^1.9.0",
    "typesafe-actions": "^1.1.2",
    "utility-types": "^1.0.0",
    "why-did-you-update": "^0.1.0"
  },
  "devDependencies": {
    "@types/enzyme": "^3.1.9",
    "@types/jest": "^22.1.2",
    "@types/react": "^16.0.38",
    "@types/react-dom": "^16.0.4",
    "@types/react-hot-loader": "^3.0.6",
    "@types/react-redux": "^5.0.15",
    "@types/react-router-dom": "^4.2.4",
    "@types/react-router-redux": "^5.0.12",
    "@types/recompose": "^0.24.5",
    "@types/webpack": "^3.8.5",
    "@types/webpack-dev-server": "^2.9.2",
    "@types/webpack-env": "^1.13.5",
    "autoprefixer": "^8.0.0",
    "css-loader": "^0.28.10",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.6",
    "html-webpack-plugin": "^2.30.1",
    "husky": "^0.14.3",
    "jest": "^22.3.0",
    "nodemon": "^1.15.1",
    "postcss-flexbugs-fixes": "^3.3.0",
    "postcss-loader": "^2.1.1",
    "react-dev-utils": "^5.0.0",
    "react-error-overlay": "^4.0.0",
    "react-hot-loader": "^3.1.3",
    "redux-freeze": "^0.1.5",
    "rimraf": "^2.6.2",
    "style-loader": "^0.20.1",
    "ts-jest": "^22.0.4",
    "ts-loader": "^3.5.0",
    "tslint": "^5.9.1",
    "tslint-react": "^3.5.1",
